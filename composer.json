{
    "name": "sarapdev/type_hint_collections",
    "description": "The Type hinted Illuminate Collections package.",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Mikhail Sarapulov",
            "email": "miha.sarapulov@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/conditionable": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/macroable": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Support\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^6.1)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
